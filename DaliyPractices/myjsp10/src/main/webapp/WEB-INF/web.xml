<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>Filter</filter-name>
        <filter-class>com.njbdqn.myjsp10.filters.Filter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>init</servlet-name>
        <servlet-class>com.njbdqn.myjsp10.controller.initCtrl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>save</servlet-name>
        <servlet-class>com.njbdqn.myjsp10.controller.saveCtrl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>single</servlet-name>
        <servlet-class>com.njbdqn.myjsp10.controller.singleCtrl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mod</servlet-name>
        <servlet-class>com.njbdqn.myjsp10.controller.modCtrl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>del</servlet-name>
        <servlet-class>com.njbdqn.myjsp10.controller.delCtrl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>del</servlet-name>
        <url-pattern>/del</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mod</servlet-name>
        <url-pattern>/mod</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>single</servlet-name>
        <url-pattern>/single</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>save</servlet-name>
        <url-pattern>/save</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>init</servlet-name>
        <url-pattern>/init</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>init</welcome-file>
    </welcome-file-list>
</web-app>
